{
    "dockerFile": "Dockerfile",
    "updateRemoteUserUID": true,
    "containerUser": "dev",
    "remoteUser": "dev",
    "postAttachCommand": "dotnet restore",
    "workspaceFolder": "",
    "runArgs": [
        "--volume=${localWorkspaceFolder}:/workspaces/${localWorkspaceFolderBasename}:Z"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp",
                "EditorConfig.EditorConfig"
            ]
        }
    }
}